{
  "version": "0.1.25",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "gen:contract": "rm -rf src/generated/smartContract/*.ts && node ./scripts/gen-contract.ts",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "alex-sdk",
  "author": "Kyle Fang",
  "module": "dist/alex-sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/alex-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/alex-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "clarity-codegen": "^0.3.5"
  },
  "peerDependencies": {
    "@stacks/network": "*",
    "@stacks/transactions": "*"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@stacks/network": "^6.3.0",
    "@stacks/stacks-blockchain-api-types": "^7.1.7",
    "@stacks/transactions": "^6.2.0",
    "@types/node": "^20.1.0",
    "esm": "^3.2.25",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
