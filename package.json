{
  "version": "2.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "prepublishOnly": "pnpm run build",
    "test": "dts test",
    "lint": "dts lint",
    "size": "size-limit",
    "gen:contract": "rm -rf src/generated/smartContract/* && tsx ./scripts/gen-contract.ts",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "alex-sdk",
  "author": "Kyle Fang",
  "module": "dist/alex-sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/alex-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/alex-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "clarity-codegen": "^0.5.2"
  },
  "peerDependencies": {
    "@stacks/network": "^6.3.0",
    "@stacks/transactions": "^6.2.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@stacks/network": "^6.3.0",
    "@stacks/stacks-blockchain-api-types": "^7.11.0",
    "@stacks/transactions": "^6.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "esm": "^3.2.25",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "size-limit": "^8.2.4",
    "dts-cli": "^2.0.5",
    "tslib": "^2.6.3",
    "tsx": "^4.15.5",
    "typescript": "^5.4.5"
  }
}
